APP_NAME="The Invitational"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=resend
MAIL_FROM_ADDRESS="info@bryantinvitational.com"
MAIL_FROM_NAME="${APP_NAME}"

RESEND_KEY=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Stripe Configuration
# Laravel Cashier uses STRIPE_KEY and STRIPE_SECRET
# These will automatically use the test/live keys based on STRIPE_ENVIRONMENT
STRIPE_KEY=
STRIPE_SECRET=

# For test environment, use either STRIPE_PUBLISHABLE_KEY/STRIPE_SECRET_KEY
# or STRIPE_TEST_KEY/STRIPE_TEST_SECRET (both work)
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_TEST_KEY=
STRIPE_TEST_SECRET=
STRIPE_LIVE_KEY=
STRIPE_LIVE_SECRET=
STRIPE_ENVIRONMENT=test

# Stripe Payment Polling
STRIPE_PAYMENT_POLL_INTERVAL=3
STRIPE_PAYMENT_POLL_MAX_ATTEMPTS=10
